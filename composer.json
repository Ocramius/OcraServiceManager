{
    "name": "ocramius/ocra-service-manager",
    "description": "ZF2 Service manager extensions to allow service proxies",
    "type": "library",
    "license": "MIT",
    "tags": [
        "zend framework",
        "lazy loading",
        "module"
    ],
    "homepage": "https://github.com/Ocramius/OcraServiceManager",
    "authors": [
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "doctrine/common": ">=2.4.x-dev"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "dev-master"
    },
    "suggest": {
        "zendframework/zend-developer-tools": "Allows displaying service dependencies"
    },
    "autoload": {
        "psr-0": {
            "OcraServiceManager\\": "src"
        }
    }
}
