{
    "name": "ocramius/ocra-service-manager",
    "description": "ZF2 Service manager extensions to allow service proxies and dependency graph generation",
    "type": "library",
    "license": "MIT",
    "tags": [
        "zend framework",
        "lazy loading",
        "module",
        "servicemanager"
    ],
    "homepage": "https://github.com/Ocramius/OcraServiceManager",
    "authors": [
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "zendframework/zend-mvc":             "2.2.*",
        "zendframework/zend-servicemanager":  "2.2.*",
        "zendframework/zend-modulemanager":   "2.2.*",
        "zendframework/zend-http":            "2.2.*",
        "doctrine/common":                    ">=2.4.x-dev,<2.5.x-dev"
    },
    "require-dev": {
        "zendframework/zendframework":        "2.2.*",
        "zendframework/zend-developer-tools": "0.*",
        "phpunit/phpunit":                    ">=3.7",
        "squizlabs/php_codesniffer":          "1.4.*",
        "satooshi/php-coveralls":             "~0.6"
    },
    "suggest": {
        "zendframework/zend-developer-tools": "Allows displaying service dependencies"
    },
    "autoload": {
        "psr-0": {
            "OcraServiceManager\\": "src"
        }
    }
}
